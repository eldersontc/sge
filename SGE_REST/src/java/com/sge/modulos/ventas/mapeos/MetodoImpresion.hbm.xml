<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.MetodoImpresion" table="MetodoImpresion" schema="Ventas">
        <id name="idMetodoImpresion" column="idMetodoImpresion">
            <generator class="sequence">
                <param name="sequence">Ventas.metodoimpresion_idmetodoimpresion_seq</param>
            </generator>
        </id>
        <property name="nombre" column="nombre"/>
        <property name="factorPases" column="factorPases"/>
        <property name="factorCambios" column="factorCambios"/>
        <property name="factorHorizontal" column="factorHorizontal"/>
        <property name="factorVertical" column="factorVertical"/>
        <property name="letras" column="letras"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>