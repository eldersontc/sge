<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.ServicioUnidad" table="ServicioUnidad" schema="Ventas">
        <id name="idServicioUnidad" column="idServicioUnidad">
            <generator class="sequence">
                <param name="sequence">Ventas.serviciounidad_idserviciounidad_seq</param>
            </generator>
        </id>
        <property name="idServicio" column="idServicio"/>
        <property name="idUnidad" column="idUnidad"/>
        <property name="abreviacionUnidad" column="abreviacionUnidad"/>
        <property name="factor" column="factor"/>
    </class>
</hibernate-mapping>