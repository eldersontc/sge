<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.Presupuesto" table="Presupuesto" schema="Ventas">
        <id name="idPresupuesto" column="idPresupuesto">
            <generator class="sequence">
                <param name="sequence">Ventas.presupuesto_idpresupuesto_seq</param>
            </generator>
        </id>
        <property name="numero" column="numero"/>
        <property name="numeracionManual" column="numeracionManual"/>
        <property name="idNumeracion" column="idNumeracion"/>
        <property name="descripcionNumeracion" column="descripcionNumeracion"/>
        <property name="idCliente" column="idCliente"/>
        <property name="razonSocialCliente" column="razonSocialCliente"/>
        <property name="idMoneda" column="idMoneda"/>
        <property name="simboloMoneda" column="simboloMoneda"/>
        <property name="fechaCreacion" column="fechaCreacion"/>
        <property name="numeroOrdenCompra" column="numeroOrdenCompra"/>
        <property name="instrucciones" column="instrucciones"/>
        <property name="estado" column="estado"/>
        <property name="total" column="total"/>
    </class>
</hibernate-mapping>