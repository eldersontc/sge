<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.ListaPrecioProducto" table="ListaPrecioProducto" schema="Ventas">
        <id name="idListaPrecioProducto" column="idListaPrecioProducto">
            <generator class="sequence">
                <param name="sequence">ventas.listaprecioproducto_idlistaprecioproducto_seq</param>
            </generator>
        </id>
        <property name="nombre" column="nombre"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>