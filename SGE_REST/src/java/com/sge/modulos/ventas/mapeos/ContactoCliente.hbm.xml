<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.ContactoCliente" table="ContactoCliente" schema="Ventas">
        <id name="idContactoCliente" column="idContactoCliente">
            <generator class="sequence">
                <param name="sequence">Ventas.contactocliente_idcontactocliente_seq</param>
            </generator>
        </id>
        <property name="idCliente" column="idCliente"/>
        <property name="nombre" column="nombre"/>
        <property name="cargo" column="cargo"/>
        <property name="telefono" column="telefono"/>
        <property name="correo" column="correo"/>
    </class>
</hibernate-mapping>