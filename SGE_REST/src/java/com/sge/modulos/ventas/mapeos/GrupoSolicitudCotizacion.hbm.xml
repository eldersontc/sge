<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.GrupoSolicitudCotizacion" table="GrupoSolicitudCotizacion" schema="Ventas">
        <id name="idGrupoSolicitudCotizacion" column="idGrupoSolicitudCotizacion">
            <generator class="sequence">
                <param name="sequence">ventas.gruposolicitudcotizacion_idgruposolicitudcotizacion_seq</param>
            </generator>
        </id>
        <property name="idSolicitudCotizacion" column="idSolicitudCotizacion"/>
        <property name="nombre" column="nombre"/>
        <property name="lineaProduccion" column="lineaProduccion"/>
        <property name="cantidad" column="cantidad"/>
    </class>
</hibernate-mapping>