<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.ServicioMaquina" table="ServicioMaquina" schema="Ventas">
        <id name="idServicioMaquina" column="idServicioMaquina">
            <generator class="sequence">
                <param name="sequence">Ventas.serviciomaquina_idserviciomaquina_seq</param>
            </generator>
        </id>
        <property name="idServicio" column="idServicio"/>
        <property name="idMaquina" column="idMaquina"/>
        <property name="descripcionMaquina" column="descripcionMaquina"/>
    </class>
</hibernate-mapping>