<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.Servicio" table="Servicio" schema="Ventas">
        <id name="idServicio" column="idServicio">
            <generator class="sequence">
                <param name="sequence">Ventas.servicio_idservicio_seq</param>
            </generator>
        </id>
        <property name="codigo" column="codigo"/>
        <property name="descripcion" column="descripcion"/>
        <property name="servicioImpresion" column="servicioImpresion"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>