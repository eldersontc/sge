<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.ventas.entidades.ServicioCotizacion" table="ServicioCotizacion" schema="Ventas">
        <id name="idServicioCotizacion" column="idServicioCotizacion">
            <generator class="sequence">
                <param name="sequence">Ventas.serviciocotizacion_idserviciocotizacion_seq</param>
            </generator>
        </id>
        <property name="idItemCotizacion" column="idItemCotizacion"/>
        <property name="idServicio" column="idServicio"/>
        <property name="descripcionServicio" column="descripcionServicio"/>
        <property name="idServicioUnidad" column="idServicioUnidad"/>
        <property name="abreviacionUnidad" column="abreviacionUnidad"/>
        <property name="precioManual" column="precioManual"/>
        <property name="precio" column="precio"/>
        <property name="cantidad" column="cantidad"/>
        <property name="total" column="total"/>
    </class>
</hibernate-mapping>