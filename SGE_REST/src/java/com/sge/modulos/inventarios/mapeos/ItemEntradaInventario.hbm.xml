<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.inventarios.entidades.ItemEntradaInventario" table="ItemEntradaInventario" schema="Inventarios">
        <id name="idItemEntradaInventario" column="idItemEntradaInventario">
            <generator class="sequence">
                <param name="sequence">Inventarios.itementradainventario_iditementradainventario_seq</param>
            </generator>
        </id>
        <property name="idEntradaInventario" column="idEntradaInventario"/>
        <property name="idProducto" column="idProducto"/>
        <property name="codigoProducto" column="codigoProducto"/>
        <property name="descripcionProducto" column="descripcionProducto"/>
        <property name="idAlmacen" column="idAlmacen"/>
        <property name="idUnidad" column="idUnidad"/>
        <property name="factor" column="factor"/>
        <property name="abreviacionUnidad" column="abreviacionUnidad"/>
        <property name="precio" column="precio"/>
        <property name="cantidad" column="cantidad"/>
        <property name="tipoCambio" column="tipoCambio"/>
        <property name="total" column="total"/>
    </class>
</hibernate-mapping>