<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.inventarios.entidades.SalidaInventario" table="SalidaInventario" schema="Inventarios">
        <id name="idSalidaInventario" column="idSalidaInventario">
            <generator class="sequence">
                <param name="sequence">Inventarios.salidainventario_idsalidainventario_seq</param>
            </generator>
        </id>
        <property name="numero" column="numero"/>
        <property name="idNumeracion" column="idNumeracion"/>
        <property name="descripcionNumeracion" column="descripcionNumeracion"/>
        <property name="numeracionManual" column="numeracionManual"/>
        <property name="idCliente" column="idCliente"/>
        <property name="razonSocialCliente" column="razonSocialCliente"/>
        <property name="idResponsable" column="idResponsable"/>
        <property name="nombreResponsable" column="nombreResponsable"/>
        <property name="fechaCreacion" column="fechaCreacion"/>
        <property name="idAlmacen" column="idAlmacen"/>
        <property name="descripcionAlmacen" column="descripcionAlmacen"/>
        <property name="idMoneda" column="idMoneda"/>
        <property name="simboloMoneda" column="simboloMoneda"/>
        <property name="idOrdenTrabajo" column="idOrdenTrabajo"/>
        <property name="numeroOrdenTrabajo" column="numeroOrdenTrabajo"/>
        <property name="observacion" column="observacion"/>
        <property name="tipoCambio" column="tipoCambio"/>
        <property name="subTotal" column="subTotal"/>
        <property name="porcentajeImpuesto" column="porcentajeImpuesto"/>
        <property name="montoImpuesto" column="montoImpuesto"/>
        <property name="total" column="total"/>
    </class>
</hibernate-mapping>