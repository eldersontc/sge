<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.administracion.entidades.Menu" table="Menu" schema="Administracion">
        <id name="idMenu" column="idMenu">
            <generator class="sequence">
                <param name="sequence">Administracion.menu_idmenu_seq</param>
            </generator>
        </id>
        <property name="idMenuPadre" column="idMenuPadre"/>
        <property name="nombre" column="nombre"/>
        <property name="formulario" column="formulario"/>
        <property name="icono" column="icono"/>
    </class>
</hibernate-mapping>