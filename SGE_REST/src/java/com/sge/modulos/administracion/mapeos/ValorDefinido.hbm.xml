<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.sge.modulos.administracion.entidades.ValorDefinido" schema="Administracion" table="ValorDefinido">
    <id column="idValorDefinido" name="idValorDefinido">
      <generator class="sequence">
        <param name="sequence">Administracion.valordefinido_idvalordefinido_seq</param>
      </generator>
    </id>
    <property column="idUsuario" name="idUsuario"/>
    <property column="usuario" name="usuario"/>
    <property column="idEntidad" name="idEntidad"/>
    <property column="json" name="json"/>
    <property column="activo" name="activo"/>
  </class>
</hibernate-mapping>
