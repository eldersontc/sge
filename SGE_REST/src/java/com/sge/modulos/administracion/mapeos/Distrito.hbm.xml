<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.administracion.entidades.Distrito" table="Distrito" schema="Administracion">
        <id name="idDistrito" column="idDistrito">
            <generator class="sequence">
                <param name="sequence">Administracion.distrito_iddistrito_seq</param>
            </generator>
        </id>
        <property name="nombre" column="nombre"/>
        <property name="idProvincia" column="idProvincia"/>
    </class>
</hibernate-mapping>