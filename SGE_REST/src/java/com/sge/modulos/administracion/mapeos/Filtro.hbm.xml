<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.administracion.entidades.Filtro" table="Filtro" schema="Administracion">
        <id name="idFiltro" column="idFiltro">
            <generator class="sequence">
                <param name="sequence">Administracion.filtro_idfiltro_seq</param>
            </generator>
        </id>
        <property name="idEntidad" column="idEntidad"/>
        <property name="nombreEntidad" column="nombreEntidad"/>
        <property name="idUsuario" column="idUsuario"/>
        <property name="usuario" column="usuario"/>
        <property name="filtro" column="filtro"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>