<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.administracion.entidades.Perfil" table="Perfil" schema="Administracion">
        <id name="idPerfil" column="idPerfil">
            <generator class="sequence">
                <param name="sequence">Administracion.perfil_idperfil_seq</param>
            </generator>
        </id>
        <property name="nombre" column="nombre"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>