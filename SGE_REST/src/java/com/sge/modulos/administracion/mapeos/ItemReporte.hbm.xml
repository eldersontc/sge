<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.administracion.entidades.ItemReporte" table="ItemReporte" schema="Administracion">
        <id name="idItemReporte" column="idItemReporte">
            <generator class="sequence">
                <param name="sequence">Administracion.itemReporte_iditemReporte_seq</param>
            </generator>
        </id>
        <property name="idReporte" column="idReporte"/>
        <property name="nombre" column="nombre"/>
        <property name="asignarId" column="asignarId"/>
        <property name="valor" column="valor"/>
    </class>
</hibernate-mapping>