<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.finanzas.entidades.ItemSalidaCaja" table="ItemSalidaCaja" schema="Finanzas">
        <id name="idItemSalidaCaja" column="idItemSalidaCaja">
            <generator class="sequence">
                <param name="sequence">finanzas.itemsalidacaja_iditemsalidacaja_seq</param>
            </generator>
        </id>
        <property name="idSalidaCaja" column="idSalidaCaja"/>
        <property name="descripcion" column="descripcion"/>
        <property name="cantidad" column="cantidad"/>
        <property name="precio" column="precio"/>
        <property name="total" column="total"/>
    </class>
</hibernate-mapping>