<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.sge.modulos.finanzas.entidades.Caja" table="Caja" schema="Finanzas">
        <id name="idCaja" column="idCaja">
            <generator class="sequence">
                <param name="sequence">finanzas.caja_idcaja_seq</param>
            </generator>
        </id>
        <property name="descripcion" column="descripcion"/>
        <property name="idMoneda" column="idMoneda"/>
        <property name="simboloMoneda" column="simboloMoneda"/>
        <property name="montoActual" column="montoActual"/>
        <property name="activo" column="activo"/>
    </class>
</hibernate-mapping>